// ActorEx1.java

import 
ch.aplu.jgamegrid.*;
import java.awt.Color;

public class Actor extends GameGrid
{
  public Actor()
  {
    super(10, 10, 60, Color.red);
    Circle nemo = new Circle();
    addActor(nemo, new Location(1, 4));
    show();
  }

  public static void main(String[] args)
  {
    new Actor();
  }
}

