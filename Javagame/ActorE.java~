// ActorEx1.java

import ch.aplu.jgamegrid.*;
import java.awt.Color;

public class ActorE extends GameGrid
{
  public ActorEx1()
  {
    super(10, 10, 60, Color.red);
    Fish nemo = new Fish();
    addActor(nemo, new Location(1, 4));
    show();
  }

  public static void main(String[] args)
  {
    new ActorEx1();
  }
}
